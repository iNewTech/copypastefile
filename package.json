{
  "name": "copypastefile",
  "displayName": "Copy Paste File",
  "description": "copy paste file to same location",
  "version": "2.1.0",
  "publisher": "iNewTech",
  "icon": "images/icon.png",
  "author": {
    "name": "Gajender Tyagi",
    "email": "sagartyagi.tyagi@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iNewTech/copypastefile"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copypastefile.duplicateFile",
        "title": "Duplicate File"
      },
      {
        "command": "copypastefile.duplicateFolder",
        "title": "Duplicate Folder"
      },
      {
        "command": "copypastefile.copyFileContentToClipboard",
        "title": "Copy File Content"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "copypastefile.duplicateFile",
          "group": "5_cutcopypaste",
          "when": "!explorerResourceIsFolder"
        },
        {
          "command": "copypastefile.duplicateFolder",
          "group": "5_cutcopypaste",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "copypastefile.copyFileContentToClipboard",
          "group": "5_cutcopypaste",
          "when": "!explorerResourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "copypastefile.duplicateFile",
          "group": "9_cutcopypaste"
        },
        {
          "command": "copypastefile.copyFileContentToClipboard",
          "group": "9_cutcopypaste"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "vscpp": "vsce package && vsce publish",
    "pre-release": "npm run package",
    "release": "vsce package -o release/ && cd release && vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "esbuild": "^0.25.8",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.4.1",
    "sinon": "^18.0.0",
    "@types/sinon": "^17.0.3"
  }
}
